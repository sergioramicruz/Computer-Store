<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <act_window id="computerstore_proveedores_action" name="PROVEEDORES"
                    res_model="computerstore.proveedores"/>

        <record id="computerstore_proveedores_view_tree" model="ir.ui.view">
            <field name="name">Lista proveedores</field>
            <field name="model">computerstore.proveedores</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="cif"/>
                    <field name="nombre"/>
                    <field name="direccion"/>
                    <field name="poblacion"/>
                    <field name="provincia"/>
                    <field name="codPostal"/>
                    <field name="telefono"/>
                    <field name="email"/>
                </tree>
            </field>
        </record>

        <record id="computerstore_proveedores_view_search" model="ir.ui.view">
            <field name="name">Busqueda proveedores</field>
            <field name="model">computerstore.proveedores</field>
            <field name="arch" type="xml">
                <search>
                    <field name="cif"/>
                    <field name="nombre"/>
                    <field name="direccion"/>
                    <field name="poblacion"/>
                    <field name="provincia"/>
                    <field name="codPostal"/>
                    <field name="telefono"/>
                    <field name="email"/>
                </search>
            </field>
        </record>
        <menuitem name="Tienda" id="computerstore_proveedores" sequence="10"/>
        <menuitem name="Inicio" id="menu_inicio" parent="computerstore_proveedores" sequence="10"/>
        <menuitem name="Proveedores" id="proveedores" action="computerstore_proveedores_action" parent="menu_inicio"
                  sequence="10"/>
    </data>
</odoo>